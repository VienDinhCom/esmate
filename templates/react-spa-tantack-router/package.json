{
  "name": "react-spa-tantack-router",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/viendinhcom/esmate",
  "scripts": {
    "dev": "esmate task dev",
    "build": "esmate task build",
    "start": "esmate task start",
    "check": "esmate task check",
    "fix": "esmate task fix"
  },
  "dependencies": {
    "@esmate/shadcn": "^1.1.2",
    "@tanstack/react-router": "^1.130.8",
    "@tanstack/react-router-devtools": "^1.130.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/router-plugin": "^1.130.8",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.32.0",
    "esmate": "^1.0.11",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "tasks": {
    "dev": "vite --host",
    "build": [
      "tsc -b",
      "vite build"
    ],
    "start": "vite preview --port 3000",
    "fix": [
      "esmate fmt",
      "esmate lint --fix"
    ],
    "check": [
      "esmate fmt --check",
      "esmate lint"
    ]
  }
}
