{
  "name": "@esmate/eslint",
  "type": "module",
  "version": "1.1.2",
  "description": "ESLint configuration for humans.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viendinhcom/esmate.git"
  },
  "keywords": [
    "esmate",
    "eslint",
    "configuration",
    "prettier",
    "typescript",
    "astro",
    "svelte",
    "react"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "fix": "prettier --write . && eslint --fix .",
    "check": "tsc && prettier --check . && eslint ."
  },
  "dependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@eslint-react/eslint-plugin": "^1.53.1",
    "@tanstack/eslint-plugin-query": "^5.90.1",
    "@tanstack/eslint-plugin-router": "^1.132.0",
    "@unocss/eslint-plugin": "^66.5.2",
    "astro-eslint-parser": "^1.2.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-format": "^1.0.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-svelte": "^3.12.4",
    "svelte-eslint-parser": "^1.3.3"
  },
  "devDependencies": {
    "@rslib/core": "^0.7.1",
    "@types/node": "^22.18.6",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
